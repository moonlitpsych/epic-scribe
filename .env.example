# Google OAuth Configuration
GOOGLE_CLIENT_ID= # Get from Google Cloud Console OAuth credentials
GOOGLE_CLIENT_SECRET= # Get from Google Cloud Console OAuth credentials

# Shared Calendar for HIPAA-compliant Meet Hosting
# Create a shared calendar in hello@trymoonlit.com account
# Share it with all doctors (with "Make changes to events" permission)
# Get Calendar ID from: Calendar Settings â†’ Integrate calendar
# Format: abc123@group.calendar.google.com
SHARED_CALENDAR_ID= # Leave empty to use user's primary calendar

# Google Workspace Configuration
GOOGLE_WORKSPACE_CUSTOMER_ID=
GOOGLE_SERVICE_ACCOUNT_EMAIL=
GOOGLE_PRIVATE_KEY=
GOOGLE_DRIVE_ROOT=/EpicScribe

# Google Gemini AI (Vertex AI or API)
GEMINI_API_KEY= # For testing - get from https://aistudio.google.com/app/apikey
GEMINI_MODEL=gemini-1.5-pro
GCP_PROJECT_ID=
GCP_REGION=us-central1
VERTEX_AI_LOCATION=us-central1

# Claude AI (Fallback)
CLAUDE_API_KEY=
CLAUDE_MODEL=claude-3.5-sonnet

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL= # Get from https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_ANON_KEY= # Get from https://app.supabase.com/project/_/settings/api
SUPABASE_SERVICE_ROLE_KEY= # Get from https://app.supabase.com/project/_/settings/api

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET= # Generate with: openssl rand -base64 32

# IntakeQ (v2.0 - Moonlit)
INTAKEQ_API_KEY=
INTAKEQ_BASE_URL=

# Feature Flags
ENABLE_PROMPT_PREVIEW=true
REQUIRE_PROMPT_PREVIEW=false
ENABLE_PHI_REDACTION=true

# Logging
LOG_LEVEL=info
LOG_PHI_REDACTED=true